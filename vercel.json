{
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "framework": "laravel",
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false"
  },
  "functions": {
    "api/index.php": {
      "runtime": "php:8.2"
    }
  },
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}
